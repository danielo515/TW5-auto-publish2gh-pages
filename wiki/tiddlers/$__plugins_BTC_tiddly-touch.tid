        "$:/plugins/BTC/tiddly-touch/widgets/muuri.js": {
            "text": "/*\\\ntitle: $:/plugins/BTC/tiddly-touch/widgets/muuri.js\ntype: application/javascript\nmodule-type: widget\n\nPackery cascading grid layout widget\n\n\\*/\n(function (global) {\n\n\"use strict\";\n/*jslint node: true, browser: true */\n/*global $tw: false */\n\nvar Widget = require(\"$:/core/modules/widgets/widget.js\").widget;\n\nif (typeof window !== 'undefined') {\n  var Muuri = require(\"$:/plugins/BTC/tiddly-touch/lib/muuri.js\");\n}\n\nvar MuuriWidget = function(parseTreeNode,options) {\n\tthis.initialise(parseTreeNode,options);\n};\n\n/*\nInherit from the base widget class\n*/\nMuuriWidget.prototype = new Widget();\n\n/*\nRender this widget into the DOM\n*/\nMuuriWidget.prototype.render = function(parent,nextSibling) {\n\tvar self = this;\n\n\t// Compute attributes and execute state\n\tthis.computeAttributes();\n\tthis.execute();\n\n  // Insert DOM node\n    var domNode = this.document.createElement(\"section\");\n  \tdomNode.setAttribute(\"class\",\"tc-muuri-grid\");\n\t  parent.insertBefore(domNode,nextSibling);\n  \tthis.domNodes.push(domNode);\n\nthis.renderChildren(domNode,null);\n\n    var targetElements = this.getAttribute(\"target\");\n    var muuriListTiddler = this.getAttribute(\"list\");\n    var targetIsTiddler = this.getAttribute(\"target-is-tiddler\") === \"yes\" ;\n    var muuriFillGaps = this.getAttribute(\"fill-gaps\") === \"yes\" ;\n    var muuriHorizontal = this.getAttribute(\"horizontal\") === \"yes\" ;\n    var muuriAlignRight = this.getAttribute(\"align-right\") === \"yes\" ;\n    var muuriAlignBottom = this.getAttribute(\"align-bottom\") === \"yes\" ;\n    var muuriEnableDrag = this.getAttribute(\"dragging\") === \"yes\" ;\nvar muuriAnimationDuration = parseInt($tw.wiki.getTiddler(\"$:/plugins/BTC/tiddly-touch/config\").getFieldString(\"animation-duration\"));\n    var muuriShowDuration = parseInt($tw.wiki.getTiddler(\"$:/plugins/BTC/tiddly-touch/config\").getFieldString(\"show-duration\"));\n    var muuriHideDuration = parseInt($tw.wiki.getTiddler(\"$:/plugins/BTC/tiddly-touch/config\").getFieldString(\"hide-duration\"));\n    var muuriDragSortAction = $tw.wiki.getTiddler(\"$:/plugins/BTC/tiddly-touch/config\").getFieldString(\"drag-sort-action\");\n    var muuriDragSortThreshold = $tw.wiki.getTiddler(\"$:/plugins/BTC/tiddly-touch/config\").getFieldString(\"drag-sort-threshold\");\n    var muuriDragSort = $tw.wiki.getTiddler(\"$:/plugins/BTC/tiddly-touch/config\").getFieldString(\"drag-sort\") === \"yes\";\n    var muuriParentWidget = this.children;\n\n    var animationDuration = $tw.utils.getAnimationDuration();\n\n    var muuri = new Muuri(domNode, {\n    items: '.' + targetElements,\n    muuriListTiddler: muuriListTiddler,\n    muuriParentWidget: muuriParentWidget,\n    targetsAreTiddlers: targetIsTiddler,\n    showDuration: muuriShowDuration,\n    hideDuration: muuriHideDuration,\n    layoutDuration: muuriAnimationDuration,\n    layoutEasing: 'cubic-bezier(0.215, 0.61, 0.355, 1)',\n    showEasing: 'cubic-bezier(0.215, 0.61, 0.355, 1)',\n    hideEasing: 'cubic-bezier(0.215, 0.61, 0.355, 1)',\n    layoutOnResize: animationDuration,\n    rounding: false,\n\t\tlayout: {\n\t\t\tfillGaps: muuriFillGaps,\n\t\t\thorizontal: muuriHorizontal,\n\t\t\talignRight: muuriAlignRight,\n\t\t\talignBottom: muuriAlignBottom,\n\t\t\trounding: false\n\t\t},\n    dragEnabled: muuriEnableDrag,\n    dragSortPredicate: {\n        threshold: muuriDragSortThreshold,\n        action: muuriDragSortAction\n  },\n    containerClass: 'tc-muuri-grid',\n    itemClass: targetElements,\n    gridMigratingClass: 'tc-grid-migrate-1',\n\t\titemVisibleClass: 'muuri-shown',\n\t\titemHiddenClass: 'muuri-hidden',\n\t\titemPositioningClass: 'muuri-positioning',\n\t\titemDraggingClass: 'tc-dragging',\n\t\titemReleasingClass: 'tc-dragover'\n  });\n\n  // add() and remove() should be invoked on the grid container by the\n  // storyview when tiddlers get inserted or removed\n  domNode.add = function(elements, options) {\n    muuri.add(elements, options);\n    muuri.hide(elements, {onFinish: function () {\n    muuri.refreshItems();\n    muuri._refreshDimensions();\n    muuri.layout();\n    muuri.show(elements);\n    muuri._settings.muuriParentWidget = self.children;\n}});\n  };\n\n  domNode.remove = function(elements, options) {\n    muuri.remove(elements, options);\n    muuri._settings.muuriParentWidget = self.children;\n  };\n\n  // Register resize event handlers for all children\n  self.registerMissingResizeListeners(domNode, muuri);\n  muuri._settings.muuriParentWidget = self.children;\n\n  // Register a handler to fire when child DOM elements are added or removed.\n  var observer = new MutationObserver(function(mutations) {\n\n    mutations.forEach(function(mutation) {\n  if (!mutation.addedNodes) return;\n\n      if (mutation.type == 'childList') {\n        //console.debug(\"MuuriWidget observed a change in childList\");\n        // Register resize event handlers for new children\n        self.registerMissingResizeListeners(domNode, muuri);\n        muuri._settings.muuriParentWidget = self.children;\n      }\n    });\n\n  });\n    \n  observer.observe(domNode, { childList: true, subtree: true, attributes: true, characterData: false});\n\n};\n\n/*\nEnsure that resize listeners are registered for all child widgets\n*/\nMuuriWidget.prototype.registerMissingResizeListeners = function(domNode, grid) {\n  var items = grid.getItems();\n  var domNodeChildren = domNode.children;\n  //grid.synchronize();\n  for (var i = 0; i < domNode.children.length; i++) {\n    var item = domNode.children[i];   \n   //console.debug(item);\n\n    if (typeof item.resizeHandler === 'undefined') {\n      item.resizeHandler = (function(item) {\n        return function() {\n          //console.debug(\"MuuriWidget got a resize event from \" + item.getAttribute('class'));\n          grid.hide(item, {\n            instant: true,\n            onFinish: function() {\n              grid.show(item, {instant: true});\n            }\n          });\n        };\n      })(item);\n\n      //console.debug(\"MuuriWidget registering for resize events from \" + item.getAttribute('class'));\n      $tw.utils.addResizeListener(item, item.resizeHandler);\n      \n      grid.add(item, { instant: true, index: i });\n          grid.hide(item, {\n            instant: true,\n            onFinish: function() {\n              grid.show(item, {instant: true, index: i });\n              items = grid.getItems();\n              if (domNodeChildren.length < items.length) {\n                grid.remove(items[i+1], { instant: true, removeElements: false });\n              grid.refreshItems();\n              grid._refreshDimensions();\n              grid.refreshSortData();\n              grid.layout(true);\n              }\n            }\n          });\n    }\n  }\n};\n\n/*\nCompute the internal state of the widget\n*/\nMuuriWidget.prototype.execute = function() {\n  this.makeChildWidgets();\n};\n\n/*\nSelectively refreshes the widget if needed. Returns true if the widget or any of its children needed re-rendering\n*/\nMuuriWidget.prototype.refresh = function(changedTiddlers) {\n  return this.refreshChildren(changedTiddlers);\n};\n\nexports.muuri = MuuriWidget;\n\n})(this);",
