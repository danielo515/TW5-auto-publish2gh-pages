        "$:/plugins/BTC/tiddly-touch/ControlPanel/settings": {
            "text": "\\define before-actions()\n<$fieldmangler tiddler=\"$:/plugins/BTC/tiddly-touch/PageTemplate/muuri\">\n<$action-sendmessage $message=\"tm-remove-tag\" $param=\"$:/tags/PageTemplate\"/></$fieldmangler><$action-setfield $tiddler=\"$:/view\" $value=\"\"/>\n\\end\n\\define after-actions()\n<$action-setfield $tiddler=\"$:/view\" $value=\"muuri\"/><$fieldmangler tiddler=\"$:/plugins/BTC/tiddly-touch/PageTemplate/muuri\"><$action-sendmessage $message=\"tm-add-tag\" $param=\"$:/tags/PageTemplate\"/></$fieldmangler>\n\\end\n\\define hide-show-leftbar-actions(setting)\n<$list filter=\"[[$:/plugins/BTC/tiddly-touch/config]get[hide-leftbar]!regexp[^yes$]]\">\n<$action-setfield $tiddler=\"$:/plugins/BTC/tiddly-touch/config\" $field=\"hide-leftbar\" $value=\"yes\"/>\n</$list>\n<$list filter=\"[[$:/plugins/BTC/tiddly-touch/config]get[hide-leftbar]regexp[^yes$]]\">\n<$action-setfield $tiddler=\"$:/plugins/BTC/tiddly-touch/config\" $field=\"hide-leftbar\" $value=\"no\"/>\n</$list>\n\\end\n\\define muuri-user-actions(setting)\n<$list filter=\"[[$:/plugins/BTC/tiddly-touch/config]get[$setting$]!regexp[^yes$]]\">\n<<before-actions>><$action-setfield $tiddler=\"$:/plugins/BTC/tiddly-touch/config\" $field=\"$setting$-user\" $value=\"yes\"/><<after-actions>>\n</$list>\n<$list filter=\"[[$:/plugins/BTC/tiddly-touch/config]get[$setting$]regexp[^yes$]]\">\n<<before-actions>><$action-setfield $tiddler=\"$:/plugins/BTC/tiddly-touch/config\" $field=\"$setting$-user\" $value=\"no\"/><$action-setfield $tiddler=\"$:/view\" $value=\"muuri\"/><<after-actions>>\n</$list>\n\\end\n\\define muuri-user-checkbox(setting,checkedunchecked,reload-yesno)\n<$set name=\"description\" value={{$:/plugins/BTC/tiddly-touch/ControlPanel/settings!!description-$setting$}}>\n<$list filter=\"[[$reload-yesno$]!regexp[^noreload$]]\">\n<$checkbox tiddler=\"$:/plugins/BTC/tiddly-touch/config\" field=\"$setting$\" $checkedunchecked$ actions=<<muuri-user-actions $setting$>>>&nbsp;&nbsp;<<description>></$checkbox>\n</$list>\n<$list filter=\"[[$reload-yesno$]regexp[^noreload$]]\">\n<$checkbox tiddler=\"$:/plugins/BTC/tiddly-touch/config\" field=\"$setting$\" $checkedunchecked$>&nbsp;&nbsp;<<description>></$checkbox>\n</$list>\n</$set>\n\\end\n\\define muuri-user-setting-field(setting)\n<$set name=\"description\" value={{$:/plugins/BTC/tiddly-touch/ControlPanel/settings!!description-$setting$}}>\n<$edit-text tiddler=\"$:/plugins/BTC/tiddly-touch/config\" field=\"$setting$\"/>&nbsp;&nbsp;<<description>>\n</$set>\n\\end\n\\define get-muuri-user-action(reload-yesno)\n<<muuri-user-checkbox \"$(listItem)$\" 'checked=\"yes\" unchecked=\"no\" '>>\n\\end\n\\define get-muuri-user-action-reverted()\n<<muuri-user-checkbox \"$(listItem)$\" 'checked=\"no\" unchecked=\"yes\" ' $reload-yesno$>>\n\\end\n\nTweak TiddlyTouch to your likings\n\n<hr>\n\n<$checkbox tiddler=\"$:/plugins/BTC/tiddly-touch/config\" field=\"sidebar-new-window\" checked=\"yes\" unchecked=\"no\">&nbsp;&nbsp;leftbar sidebar button (top right button will behave the standard way): open sidebar in new window on/off</$checkbox>\n\n<$list filter=\"[list[$:/plugins/BTC/tiddly-touch/ControlPanel/settings!!user-checkboxes]]\" variable=\"listItem\">\n\n<<get-muuri-user-action>>\n\n</$list>\n\n<$list filter=\"[list[$:/plugins/BTC/tiddly-touch/ControlPanel/settings!!user-checkboxes-noreload]]\" variable=\"listItem\">\n\n<<get-muuri-user-action noreload>>\n\n</$list>\n\n<$list filter=\"[list[$:/plugins/BTC/tiddly-touch/ControlPanel/settings!!user-checkboxes-reverted]]\" variable=\"listItem\">\n\n<<get-muuri-user-action-reverted>>\n\n</$list>\n\n<$checkbox tiddler=\"$:/plugins/BTC/tiddly-touch/PageTemplate/LeftBar\" tag=\"$:/tags/PageTemplate\" invertTag=\"yes\" actions=<<hide-show-leftbar-actions>>>&nbsp;&nbsp;hide the left bar</$checkbox>\n\n<<muuri-user-checkbox \"drag-sort-action\" ' checked=\"swap\" unchecked=\"move\" '>>\n\n> For the following settings to take effect a muuri reload is needed. The simplest way to do that is switching to the normal view and back\n\n<br>\n\n<<muuri-user-setting-field \"show-duration\">>\n\n<<muuri-user-setting-field \"hide-duration\">>\n\n<<muuri-user-setting-field \"animation-duration\">>\n\n",
